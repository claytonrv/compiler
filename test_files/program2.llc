def abs(valor){
    if(valor > 0){
        return valor;
    }
    return 0;
}

def sqrt(valor){
    return valor ** 2
}

def pow(valor, potencia){
    int valorFinal = 0;
    for(int i=0;i<potencia;i++){
        valorFinal = valorFinal + (valor * valor);
    }
    return valorFinal;
}

double f(double t)
{
    return sqrt(abs(t)) + 5 * pow(t, 3);
}

